<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>BuildTester.Console</id>
    <version>${Version}</version>
    <authors>P. van der Velde</authors>
    <owners>P. van der Velde</owners>
    <licenseUrl>http://www.apache.org/licenses/LICENSE-2.0</licenseUrl>
    <projectUrl>https://github.com/pvandervelde/BuildTester</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>
      The output of a repository that is used for testing build scripts and interaction with GitHub.
    </description>
    <releaseNotes>
      ${ReleaseNotes}
    </releaseNotes>
    <dependencies>
      ${Dependencies}
    </dependencies>
  </metadata>
  <files>
    <file src="${DirBin}\BuildTester.Console.exe" target="lib\net45" />
    <file src="${DirBin}\BuildTester.Console.pdb" target="lib\net45" />
    <file src="${DirBin}\BuildTester.Console.xml" target="lib\net45" />
    <file src="${DirSrc}\console\**\*.cs" target="src\console" />
  </files>
</package>